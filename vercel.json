{
  "version": 2,
  "builds": [
    {
      "src": "web/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/function-config",
      "dest": "/index.html"
    },
    {
      "src": "/function-redirect",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
